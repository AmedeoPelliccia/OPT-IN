{
  "$schema":"http://json-schema.org/draft-07/schema#",
  "schema_version":"1.0",
  "title":"Waste Tank",
  "type":"object",
  "required":["id","part_number","capacity_l","material","service_pressure_bar"],
  "properties":{
    "id":{"type":"string"},
    "part_number":{"type":"string"},
