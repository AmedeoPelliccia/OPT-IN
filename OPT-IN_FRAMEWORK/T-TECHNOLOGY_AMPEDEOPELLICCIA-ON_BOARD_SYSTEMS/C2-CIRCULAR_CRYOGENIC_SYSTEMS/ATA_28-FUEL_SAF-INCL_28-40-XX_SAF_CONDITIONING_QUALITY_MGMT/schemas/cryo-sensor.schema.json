{
  "$schema":"http://json-schema.org/draft-07/schema#",
  "schema_version":"1.0",
  "title":"Cryogenic Sensor Specification",
  "type":"object",
  "required":["id","part_number","sensor_type","measurement_range","accuracy"],
  "properties":{
    "id":{"type":"string"},
    "part_number":{"type":"string"},
    "sensor_type":{"type":"string"},
    "measurement_range":{"type":"array","items":{"type":"number"}},
    "accuracy":{"type":"string"},
    "operating_temp_range_C":{"type":"array","items":{"type":"number"}},
    "calibration_interval_months":{"type":"number"},
    "output_signal":{"type":"string"},
    "material":{"type":"string"},
    "coc":{"type":"string"}
  }
}
